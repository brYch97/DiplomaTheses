<?xml version="1.0" encoding="utf-8" ?>
<manifest>
  <control namespace="Sample" constructor="GoogleMapsGrid" version="0.0.2" display-name-key="GoogleMapsGrid_Display_Key" description-key="GoogleMapsGrid_Desc_Key" control-type="standard"  preview-image="images/gmaps_thumb.png">
    <!-- dataset node represents a set of entity records on CDS; allow more than one datasets -->
    <data-set 
        name="mapDataSet" 
        display-name-key="Contacts dataset">
      <property-set 
        name="latitude" 
        display-name-key="Latitude" 
        description-key="Column containing latitude value." 
        of-type="FP" 
        usage="bound" 
        required="true" />
      <property-set 
        name="longitude" 
        display-name-key="Longitude" 
        description-key="Column containing longitude value." 
        of-type="FP" 
        usage="bound" 
        required="true" />
      <property-set 
        name="name" 
        display-name-key="Primary name field" 
        description-key="Column containing the record's primary name." 
        of-type="SingleLine.Text" 
        usage="bound" 
        required="true" />
    </data-set>
    <property name="googleMapsAPIKey" display-name-key="Google Maps API Key" description-key="API key for Google Maps." of-type="SingleLine.Text" usage="input" required="true"  />
    <resources>
      <code path="index.ts" order="1"/>
      <css path="css/GoogleMapsGrid.css" order="1" />
      <resx path="strings/GoogleMapsGrid.1033.resx" version="1.0.0" />
    </resources>
  </control>
</manifest>