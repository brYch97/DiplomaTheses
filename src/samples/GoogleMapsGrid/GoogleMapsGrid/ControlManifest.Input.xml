<?xml version="1.0" encoding="utf-8" ?>
<manifest>
  <control namespace="Sample" constructor="GoogleMapsGrid" version="0.0.2" display-name-key="GoogleMapsGrid_Display_Key" description-key="GoogleMapsGrid_Desc_Key" control-type="standard"  preview-image="images/gmaps_thumb.png">
    <!-- dataset node represents a set of entity records on CDS; allow more than one datasets -->
    <data-set name="mapDataSet" display-name-key="mapDataSet_Display_Key">
      <!-- 'property-set' node represents a unique, configurable property that each record in the dataset must provide. -->
      <!-- UNCOMMENT TO ADD PROPERTY-SET NODE
        
      <property-set name="samplePropertySet" display-name-key="Property_Display_Key" description-key="Property_Desc_Key" of-type="SingleLine.Text" usage="bound" required="true" />
      -->
      <property-set name="latitude" display-name-key="Latitude field" description-key="Property_Desc_Key" of-type="FP" usage="bound" required="true" />
      <property-set name="longitude" display-name-key="Longitude field" description-key="Property_Desc_Key" of-type="FP" usage="bound" required="true" />
      <property-set name="name" display-name-key="Primary name field" description-key="Property_Desc_Key" of-type="SingleLine.Text" usage="bound" required="true" />
    </data-set>
    <property name="googleMapsAPIKey" display-name-key="MapGrid_googleMapsAPIKey_Display_Key" description-key="MapGrid_googleMapsAPIKey_Desc_Key" of-type="SingleLine.Text" usage="input" required="true"  />
    <resources>
      <code path="index.ts" order="1"/>
      <css path="css/GoogleMapsGrid.css" order="1" />
      <resx path="strings/GoogleMapsGrid.1033.resx" version="1.0.0" />
    </resources>
  </control>
</manifest>